{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"noEmit": true,
		"jsx": "preserve",
		"module": "ESNext",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./src/*"]
		},
		"allowJs": true,
		"incremental": true
	},
	"include": ["src", "next-env.d.ts", "*.config.ts", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
